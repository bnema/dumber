{
  "name": "@dumber/gui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:gui && npm run build:homepage",
    "build:gui": "vite build --config vite.config.gui.ts",
    "build:homepage": "vite build --config vite.config.homepage.ts",
    "dev": "npm run build:gui -- --watch",
    "dev:homepage": "npm run build:homepage -- --watch",
    "check": "svelte-check",
    "lint": "eslint src --fix",
    "generate:types": "node ../scripts/generate-types.js"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/cli": "^4.0.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "postcss": "^8.4.47",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.43.0",
    "vite": "^5.0.0"
  }
}
